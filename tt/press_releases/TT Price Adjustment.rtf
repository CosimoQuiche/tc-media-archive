\par \pard\plain \s2\rtlch\af7\afs36\ab \ltrch\hich\af3\loch\ilvl1\outlinelevel1\sb200\sa120\f3\fs36\b\dbch\af6\sl276\slmult1\ql\sb0\sa140\ltrpar{\loch
